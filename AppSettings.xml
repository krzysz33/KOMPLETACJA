    <SqlStatment>
      <Name>Kontrah</Name>
      <DefoultSearching>NazwaSkr</DefoultSearching>
      <Sql>
      SELECT k.id_kontrah, k.id_defceny, k.nrkontrah, k.nazwaskr, dk.nazwadl, dk.nip, 0 as Zazn FROM kontrah k
        JOIN danekontrah dk ON (k.id_kontrah = dk.id_kontrah AND dk.bazadanekontrah = 1)
        JOIN wystgrktr WG on WG.id_kontrah = K.id_kontrah and Wg.id_rodzgrupktr = 1
        join grupakontrah gk on gk.id_grupakontrah = wg.id_grupakontrah
        where gk.id_grupakontrah  = 10003
      </Sql>
